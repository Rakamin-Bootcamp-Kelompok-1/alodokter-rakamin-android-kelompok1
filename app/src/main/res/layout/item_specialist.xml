<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:padding="@dimen/spacing_5">

    <ImageView
        android:id="@+id/ivSpecialistDoctor"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_dokter_umum"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/tvSpecialistDoctor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dokter Umum"
        android:fontFamily="@font/nunito_bold"
        app:layout_constraintTop_toBottomOf="@+id/ivSpecialistDoctor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_margin="@dimen/spacing_5"
        />

</androidx.constraintlayout.widget.ConstraintLayout>